{
  "name": "@gasbuddy/react-navigation-codegen",
  "version": "6.0.0",
  "description": "Type safe code generation for react-navigation based apps",
  "main": "build/index.js",
  "bin": "build/cli.js",
  "types": "build/index",
  "files": [
    "build"
  ],
  "scripts": {
    "clean-build": "yarn run clean && yarn run build",
    "clean": "rm -rf ./build",
    "build": "tsc -p tsconfig.json && chmod +x build/cli.js",
    "test": "tap --ts --no-coverage tests/*.ts",
    "cover": "nyc --reporter=lcov npm test",
    "lint": "eslint .",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:gas-buddy/react-navigation-codegen.git"
  },
  "keywords": [
    "react-native",
    "react-navigation",
    "typescript",
    "gasbuddy"
  ],
  "author": "GasBuddy",
  "license": "MIT",
  "devDependencies": {
    "@react-native-community/eslint-config": "^3.2.0",
    "@types/js-yaml": "^4.0.5",
    "@types/minimist": "^1.2.0",
    "@types/node": "^18.11.9",
    "@types/prettier": "^2.0.2",
    "@types/tap": "^15.0.7",
    "@typescript-eslint/parser": "^5.42.0",
    "cross-env": "^7.0.0",
    "eslint": "^8.27.0",
    "tap": "^16.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "nyc": {
    "lines": 75,
    "statements": 75,
    "functions": 75,
    "branches": 75,
    "include": [
      "src"
    ],
    "require": [
      "@babel/register"
    ],
    "sourceMap": true,
    "reporter": [
      "lcov"
    ]
  },
  "dependencies": {
    "js-yaml": "^4.1.0",
    "minimist": "^1.2.5",
    "mkdirp": "^3.0.1",
    "prettier": "^2.1.1"
  }
}
