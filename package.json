{
  "name": "@gasbuddy/react-navigation-codegen",
  "version": "1.0.0",
  "description": "Type safe code generation for react-navigation based apps",
  "main": "build/index.js",
  "bin": "build/cli.js",
  "types": "build/index",
  "files": [
    "build"
  ],
  "scripts": {
    "clean-build": "yarn run clean && yarn run build",
    "clean": "rm -rf ./build",
    "build": "tsc -p tsconfig.json",
    "test": "tap --no-coverage tests/*.ts",
    "cover": "nyc --reporter=lcov npm test",
    "lint": "eslint .",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:gas-buddy/react-navigation-codegen.git"
  },
  "keywords": [
    "react-native",
    "react-navigation",
    "typescript",
    "gasbuddy"
  ],
  "author": "GasBuddy",
  "license": "MIT",
  "devDependencies": {
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/js-yaml": "^3.12.5",
    "@types/minimist": "^1.2.0",
    "@types/node": "^14.0.23",
    "@types/prettier": "^2.0.2",
    "@types/tap": "^14.10.0",
    "@typescript-eslint/parser": "^3.6.1",
    "cross-env": "^7.0.0",
    "eslint": "^7.4.0",
    "tap": "^14.10.7",
    "typescript": "^3.9.5"
  },
  "nyc": {
    "lines": 75,
    "statements": 75,
    "functions": 75,
    "branches": 75,
    "include": [
      "src"
    ],
    "require": [
      "@babel/register"
    ],
    "sourceMap": true,
    "reporter": [
      "lcov"
    ]
  },
  "dependencies": {
    "js-yaml": "^3.14.0",
    "minimist": "^1.2.5",
    "prettier": "^2.0.5"
  }
}
